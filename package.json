{
  "name": "tmux-session-manager",
  "version": "0.1.0",
  "description": "Tmux Session Manager for feature development (Node + Ink)",
  "bin": {
    "dev-sessions": "dist/bin/dev-sessions.js"
  },
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsc -p .",
    "typecheck": "tsc --noEmit",
    "start": "node dist/index.js",
    "cli": "node dist/bin/dev-sessions.js"
  },
  "dependencies": {
    "ink": "^4.4.1",
    "react": "^18.3.1"
  },
  "devDependencies": {
    "@types/node": "^22.5.4",
    "@types/react": "^18.3.3",
    "typescript": "^5.5.4"
  }
}
