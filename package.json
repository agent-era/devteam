{
  "name": "@agent-era/devteam",
  "version": "1.0.0",
  "description": "A terminal-based developer AI manager for parallel feature development with Claude Code, Codex, and Gemini CLI",
  "keywords": [
    "tmux",
    "git",
    "worktree",
    "development",
    "agent-manager",
    "cli",
    "session-manager",
    "feature-branches",
    "claude",
    "claude-code",
    "codex",
    "gemini"
  ],
  "bin": {
    "devteam": "dist/bin/devteam.js"
  },
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc -p .",
    "typecheck": "npm install && tsc --noEmit",
    "start": "node dist/index.js",
    "cli": "node dist/bin/devteam.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:terminal": "npm run build && tsc -p tsconfig.node-tests.json && node --test tests/e2e/terminal/*.test.mjs",
    "prepublishOnly": "npm run build && npm run typecheck && npm test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/agent-era/devteam.git"
  },
  "homepage": "https://github.com/agent-era/devteam#readme",
  "bugs": {
    "url": "https://github.com/agent-era/devteam/issues"
  },
  "author": "Agent Era",
  "license": "MIT",
  "dependencies": {
    "@inkjs/ui": "^2.0.0",
    "ink": "^5.2.1",
    "ink-syntax-highlight": "^2.0.2",
    "react": "^18.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.4",
    "@testing-library/react": "^14.3.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.5.4",
    "@types/react": "^18.3.3",
    "babel-jest": "^29.7.0",
    "ink-testing-library": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.1.1",
    "jest-environment-node": "^29.7.0",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  }
}
